<UserControl x:Class="Final_project.Views.SettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Final_project.Views" 
             xmlns:components="clr-namespace:Final_project.Components"
             xmlns:viewmodels="clr-namespace:Final_project.ViewModels" 
             d:DataContext="{d:DesignInstance Type=viewmodels:SettingsVM}"
             mc:Ignorable="d" 
             Background="Yellow"
             d:DesignHeight="450" d:DesignWidth="800">

        <Grid >
              <TextBlock FontSize="33"
                         HorizontalAlignment="Center"
                         VerticalAlignment="Top"
                         Text="Setting" Margin="0,20,0,0"/>

        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="300">
            <Button  FontSize="23" Content="Go to Login" Command="{Binding NavigateLogin}"/>
        </StackPanel>
    </Grid>
  
</UserControl>
